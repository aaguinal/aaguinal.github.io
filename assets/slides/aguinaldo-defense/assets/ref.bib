@article{Freund2002,
abstract = {This paper presents a novel system that allows to generate variants of industrial robot programs. Such a system is widely needed in different areas of application. First, product scenarios change often slightly and thereby cause high programming costs. Using our system reduces these costs, because non-specialists can create a specific program variant by themselves. Second, an engineer needs numerous program variants to find an optimized cell layout, e.g. regarding robot execution time. Present systems require too much manual effort and they are very limited in considering application-specific requirements. Consequently, we have implemented a system that contains an interpreter to execute user-defined scripts. A script contains the information on how to generate the program variants. Each change of the parameter values inside the script results in a different program. We simulate the program execution and use the simulation results to further improve the programs. We prove the usefulness of our system by industrial applications.},
annote = {This paper provides a system that generates robot program variants in controller-specific syntax. 

The requirements of a system that generates robot program variants:
1. Be able to extract geometric information
2. Be able to define algorithms that operate on geometric information
3. Must include a controller independent specification of the desired sequence of robot commands (**i.e. task-based language**)
4. Be able to test programs in simulation environment
5. Have an open interface to allow extending the system

The generator system contains (4) layers:
* Application layer
- Contains input assistant that allows the user to generate one specific program variant
- Another part to generate all possible program variants which includes information like:
$\sim$ generation plans: script that contains all commands to generate the program variants
$\sim$ syntax definition (e.g. for KUKA KRL or MELFA BASIC)
$\sim$ technology package (e.g. KUKA ArcTech and ABB ArcWare)
* Generation layer
- Generation controller: execute generation plan
- Program synthesis: reads the syntax definition and incorporates the program from the generation controller to create controller specific program
* Simulation layer
- Simulation model: contains the geometric information of work cell
- Virtual robot controller: take controller specific program and executes
- Simulation results
* Manufacturing layer (on real hardware)


More about the generation plans:
Plan language included with this paper. Requirements of the generation plan are:
- Plan variables (data types and map to syntax)
- Plan steps (define instructions using plan variables)

To apply the generation plan to a problem, you must describe in the plan:
- Variables containing geometric information
- Plan steps to calculate things from the geometric information
- Sequence of instructions to execute

After generating the robot program, the program is executed in simulation and checked for collisions, etc.

**Where is the logic defined?**},
author = {Freund, Eckhard and Luedemann-Ravit, Bernd},
doi = {10.1109/irds.2002.1044026},
file = {:Users/angelineaguinaldo/Documents/Mendeley Desktop/Freund, Luedemann-Ravit/2002/IEEE International Conference on Intelligent Robots and Systems/Freund, Luedemann-Ravit - 2002 - A system to automate the generation of program variants for industrial robot applications.pdf:pdf},
journal = {IEEE International Conference on Intelligent Robots and Systems},
number = {October},
pages = {1856--1861},
title = {{A system to automate the generation of program variants for industrial robot applications}},
volume = {2},
year = {2002}
}
@book{Elimelech2023,
abstract = {Solving realistic robotic task planning problems is computationally demanding. To better exploit the planning effort and reduce the future planning cost, it is important to increase the reusability of successful plans. To this end, we suggest a systematic and automatable approach for plan transfer, by rethinking the plan caching procedure. Specifically, instead of caching successful plans in their original domain, we suggest transferring them upon discovery to a dynamically-defined abstract domain and cache them as “abstract skills” there. This technique allows us to maintain a unified, standardized, and compact skill database, to avoid skill redundancy, and to support lifelong operation. Cached skills can later be reconstructed into new domains on demand, and be applied to new tasks, with no human intervention. This is made possible thanks to the novel concept of “abstraction keys.” An abstraction key, when coupled with a skill, provides all the necessary information to cache it, reconstruct it, and transfer it across all domains in which it is applicable—even domains we have yet to encounter. We practically demonstrate the approach by providing two examples of such keys and explain how they can be used in a manipulation planning domain.},
author = {Elimelech, Khen and Kavraki, Lydia E. and Vardi, Moshe Y.},
booktitle = {Springer Proceedings in Advanced Robotics},
doi = {10.1007/978-3-031-21090-7_28},
file = {:Users/angelineaguinaldo/Documents/Mendeley Desktop/Elimelech, Kavraki, Vardi/2023/Springer Proceedings in Advanced Robotics/Elimelech, Kavraki, Vardi - 2023 - Automatic Cross-domain Task Plan Transfer by Caching Abstract Skills.pdf:pdf},
isbn = {9783031210891},
issn = {25111264},
pages = {470--487},
publisher = {Springer International Publishing},
title = {{Automatic Cross-domain Task Plan Transfer by Caching Abstract Skills}},
url = {http://dx.doi.org/10.1007/978-3-031-21090-7_28},
volume = {25 SPAR},
year = {2023}
}
