@article{Morak2020,
abstract = {Checking whether action effects can be undone is an important question for determining, for instance, whether a planning task has dead-ends. In this paper, we investigate the reversibility of STRIPS actions, that is, when the effects of an action can be reverted by applying other actions, in order to return to the original state. We propose a broad notion of reversibility that generalizes previously defined versions and investigate interesting properties and relevant restrictions. In particular, we propose the concept of uniform reversibility that guarantees that an action can be reverted independently of the state in which the action was applied, using a socalled reverse plan. In addition, we perform an in-depth investigation of the computational complexity of deciding action reversibility. We show that reversibility checking with polynomial-length reverse plans is harder than polynomiallength planning and that, in case of unrestricted plan length, the PSPACE-hardness of planning is inherited. In order to deal with the high complexity of solving these tasks, we then propose several incomplete algorithms that may be used to compute reverse plans for a relevant subset of states.},
author = {Morak, Michael and Chrpa, Luk{\'{a}}{\v{s}} and Faber, Wolfgang and Fi{\v{s}}er, Daniel},
doi = {10.24963/kr.2020/65},
file = {:Users/angelineaguinaldo/Documents/Mendeley Desktop/Morak et al/2020/17th International Conference on Principles of Knowledge Representation and Reasoning, KR 2020/Morak et al. - 2020 - On the reversibility of actions in planning.pdf:pdf},
isbn = {9781713825982},
journal = {17th International Conference on Principles of Knowledge Representation and Reasoning, KR 2020},
number = {Kr},
pages = {651--660},
title = {{On the reversibility of actions in planning}},
volume = {2},
year = {2020}
}
