eprint = {2210.12631},
file = {:Users/angelineaguinaldo/Documents/Mendeley Desktop/Cheng, Xu/2023/IEEE Robotics and Automation Letters/Cheng, Xu - 2023 - LEAGUE Guided Skill Learning and Abstraction for Long-Horizon Manipulation.pdf:pdf},
issn = {23773766},
journal = {IEEE Robotics and Automation Letters},
keywords = {Reinforcement learning,continual learning,task and motion planning},
number = {10},
pages = {6451--6458},
publisher = {IEEE},
title = {{LEAGUE: Guided Skill Learning and Abstraction for Long-Horizon Manipulation}},
volume = {8},
year = {2023}
}
