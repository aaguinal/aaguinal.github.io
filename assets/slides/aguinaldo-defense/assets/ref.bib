keywords = {AI planning,Hierarchical task networks,Web service composition},
pages = {124--156},
publisher = {Elsevier B.V.},
title = {{HTN planning: Overview, comparison, and beyond}},
url = {http://dx.doi.org/10.1016/j.artint.2015.02.002},
volume = {222},
year = {2015}
}
