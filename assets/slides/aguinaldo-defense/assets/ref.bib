mendeley-groups = {Angeline's PhD Dissertation/Chp2.Background/2.Planning/B.Task and Motion Planning},
pages = {4911--4918},
title = {{Increased mars rover autonomy using AI planning, scheduling and execution}},
year = {2007}
}

@inproceedings{Bøgh2012,
abstract = {This article presents a unifying terminology for task-level programming of highly flexible mobile manipulators in industrial environments. With a terminology of tasks and object-centered skills, industrial applications are analyzed in the logistic and assistive domains. The analysis shows that many tasks can actually be solved with a small library of predefined building blocks, called skills. In logistics domains, it can be exploited that the sequences of skills needed to solve specific tasks follow certain patterns, whereas in assistive tasks, the sequence vary a lot, and tasks must be taught to the robot by a user, based on Standard Operating Procedures or process knowledge. An overview is presented on how to implement a skill-based architecture, enabling reuse of skills for different industrial applications, programmed by shop-floor workers. The terminology of tasks, skills, and motion primitives is introduced and designed to separate responsibilities of robot system developers, robot system integrators and shop floor users concerning programming of the robot system. The concept of testable pre- and postconditions assigned to the skills are proposed to help both in developing skills and asserting when a skill is applicable.},
author = {B{\o}gh, Simon and Nielsen, Oluf Skov and Pedersen, Mikkel Rath and Kr{\"{u}}ger, Volker and Madsen, Ole},
booktitle = {Proceedings of the 43rd International Symposium on Robotics},
file = {:Users/angelineaguinaldo/Documents/Mendeley Desktop/B{\o}gh et al/2012/Proceedings of the 43rd International Symposium on Robotics/B{\o}gh et al. - 2012 - Does your Robot have Skills.pdf:pdf},
keywords = {ble automation,flexi-,industrial robots,mobile manipulation,robot skills,task-level programming},
title = {{Does your Robot have Skills ?}},
year = {2012}
}
@article{Jiang2019,
abstract = {Robots need task planning algorithms to sequence actions toward accomplishing goals that are impossible through individual actions. Off-the-shelf task planners can be used by intelligent robotics practitioners to solve a variety of planning problems. However, many different planners exist, each with different strengths and weaknesses, and there are no general rules for which planner would be best to apply to a given problem. In this study, we empirically compare the performance of state-of-the-art planners that use either the planning domain description language (PDDL) or answer set programming (ASP) as the underlying action language. PDDL is designed for task planning, and PDDL-based planners are widely used for a variety of planning problems. ASP is designed for knowledge-intensive reasoning, but can also be used to solve task planning problems. Given domain encodings that are as similar as possible, we find that PDDL-based planners perform better on problems with longer solutions, and ASP-based planners are better on tasks with a large number of objects or tasks in which complex reasoning is required to reason about action preconditions and effects. The resulting analysis can inform selection among general-purpose planning systems for particular robot task planning domains.},
archivePrefix = {arXiv},
arxivId = {1804.08229},
author = {qian Jiang, Yu and qi Zhang, Shi and Khandelwal, Piyush and Stone, Peter},
doi = {10.1631/FITEE.1800514},
eprint = {1804.08229},
file = {:Users/angelineaguinaldo/Documents/Mendeley Desktop/Jiang et al/2019/Frontiers of Information Technology and Electronic Engineering/Jiang et al. - 2019 - Task planning in robotics an empirical comparison of PDDL- and ASP-based systems.pdf:pdf},
issn = {20959230},
journal = {Frontiers of Information Technology and Electronic Engineering},
keywords = {Answer set programming (ASP),Planning domain description language (PDDL),Robotics,TP242,Task planning},
number = {3},
pages = {363--373},
title = {{Task planning in robotics: an empirical comparison of PDDL- and ASP-based systems}},
volume = {20},
year = {2019}
}
@inproceedings{Zhang2023a,
annote = {SUMMARY 

The paper presents an approach to integrate AI planning technologies with robotic applications using a domain-independent task representation language TAEMS aimed at combining high-level deliberation with low-level uncertainty. Some notable features of TAEMS are:

* The ability to aggregate probabilistic quality, cost, and duration scores after a task or subtask has been completed. The authors state that these semantics for these scores can be assigned domain-specific meaning at the time of modeling.
* The addition of non-local effects which act as constraints between subtasks and methods which loosens the underlying data structure from a tree to a directed graph. 

The paper also introduces domain-independent tools that can generate a schedule of executable methods for the robot and handle necessary collaboration with other robots based on TAEMS framework. Provided this representation, the an intelligent scheduler would be responsible for choosing which among multiple schedules (plans) within the task network optimizes quality, cost, and/or duration. The drawback to this approach is that the modeling of the task network would not be provided automatically, but would instead require a user to model it.

The paper also discusses the challenges of integrating high-level reasoning and low-level functional behaviors for the robot, such as when to interrupt, replan, or communicate. The paper also describes a platform for developing software agents using TAEMS and testing them in a simulated environment with real-world physics, using JADE framework and LibGDX library.


STRENGTHS 

* Clarity of writing: The figures were very nicely generated. Figure 2 along with the written explanation was particularly helpful for understanding the TAEMS representation.
* Research Contribution: The addition of non-local effects is beneficial for modeling dependencies between subtasks and methods. They are effectively encoding the success of previous methods as preconditions for other subtasks and methods.
* Claims: The discussion regarding the robot control challenges for high-level and low-level controller integration provide accurate expositions with good examples.

WEAKNESSES

* Clarity of writing: There are a few typos that were found upon reading the paper:
- Page 2 (TAEMS Formal Representation of Tasks and Environment): "TAEMS example in graphically in" --> "TAEMS example graphically in"
- Page 6 (Conclusion and Future Work): "Robotcup resue" --> "RoboCup Rescue" (?)
* Potential for Impact/Usefulness: This appears to be another syntactic formalism for modeling hierarchical task plans with some extensions, such as the addition of non-local effects. With each added feature, the complexity of the scheduling algorithm could increase. This paper could be improved by answering questions, such as how would existing or novel planners use this representation? What AI technologies would they use? The specific AI technologies are not mentioned in the paper.
* Claims: This paper begs a few clarifying details about the scheduler that are preventing the novel contributions from being pronounced. When does the quality, cost, and duration get updated? Do these values can computed at execution time, or does the scheduler assign expected values for these attributes and then passes along this task plan for execution? Additionally, how does the task network get updated if other subtasks and methods need to be introduced? More emphasis could have been placed on these details and less on the high-level to low-level controller integration and simulator. These are known problems that the novel task representation language is not directly addressing these issues in this paper.},
author = {Zhang, Shelley and Moulton, Alexander and Bedi, Abhijot and Chabot, Eugene},
booktitle = {Vol. 2 No. 1: Proceedings of the 2023 AAAI Fall Symposia / Unifying Representations for Robot Application Development},
file = {:Users/angelineaguinaldo/Documents/Mendeley Desktop/Zhang et al/2023/Vol. 2 No. 1 Proceedings of the 2023 AAAI Fall Symposia Unifying Representations for Robot Application Development/Zhang et al. - 2023 - Supporting AI Planning and Collaboration for Robotic Applications Using T{\AE}MS.pdf:pdf},
title = {{Supporting AI Planning and Collaboration for Robotic Applications Using T{\AE}MS}},
year = {2023}
}
@article{Bottazzi2005,
abstract = {The industrial robot programming is a work for specialist in robotics. Today, this work is very hard because there are many robot manufacturers with different languages and different programming environments. Although, off-line programming is an way that can reduce drastically the machines stop time to maintenance. With the use of object oriented design patterns, it is possible minimize the time spent in robot programming. In this work is proposed a off-line programming environment. This tool is based in one abstract model to program robots, encapsulate in Java classes. This way has the main advantage of best source code utilization. Grouping the business classes in modules by functionalities, we can reduce complexity between low matching. Recognized patterns like Facade and Template Method will construct the base to develop this programming framework. The programming robot languages tested in this work was Rapid, Karel and Melfa Basic IV, respectively languages used by ABB, Fanuc and Mitsubishi constructors. {\textcopyright} 2005 IEEE.},
