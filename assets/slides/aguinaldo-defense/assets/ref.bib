@article{Daclin2017,
abstract = {This chapter presents research works within the field of interoperability measurement. It outlines the set of interoperability measurement components, such as potentiality, compatibility and performance measurement. The chapter provides some running examples to better understand the metrics. Potentiality means that an enterprise owns intrinsic attributes related to the categories of interoperability (use of standard applications, enterprise models, database, etc.), allowing easy interoperability in the eventuality of a partnership. Interoperability degree gives information to partners about the presence (or absence) of incompatibilities disturbing the interoperability. This evaluation allows partners to avoid and/or correct deficiencies during the collaboration. The interoperability degree includes compatibility measurement and performance measurement. Compatibility measurement is done during the engineering stage, that is, when systems need to set up interoperability with a known partner. The performance measurement of interoperability allows enterprises to evaluate interoperability according to a set of predefined criteria.},
author = {Daclin, Nicolas and Chen, David and Vallespir, Bruno},
doi = {10.1002/9781119407928.ch6},
file = {:Users/angelineaguinaldo/Documents/Mendeley Desktop/Daclin, Chen, Vallespir/2017/Enterprise Interoperability INTEROP-PGSO Vision/Daclin, Chen, Vallespir - 2017 - The interoperability measurement.pdf:pdf},
isbn = {9781119407928},
journal = {Enterprise Interoperability: INTEROP-PGSO Vision},
keywords = {Compatibility measurement,Interoperability measurement,Performance measurement,Potentiality measurement},
pages = {107--129},
title = {{The interoperability measurement}},
year = {2017}
}
