journal = {Robotics and Computer-Integrated Manufacturing},
keywords = {Agility,Knowledge representation,PDDL (planning domain definition language),Planning,Replanning,Robotics},
pages = {42--55},
publisher = {Elsevier},
title = {{Towards robust assembly with knowledge representation for the planning domain definition language (PDDL)}},
url = {http://dx.doi.org/10.1016/j.rcim.2014.08.006},
volume = {33},
year = {2015}
}
