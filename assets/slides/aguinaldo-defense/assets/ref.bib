@article{Cowley2011,
abstract = {The decomposition of robotics software into a collection of loosely coupled processes has become a core design principle of virtually every large robotics software engineering effort over the past decade. Recently, the ROS software platform from Willow Garage has gained significant traction due to its adoption of sound design principles and significant software library contributions from Willow Garage itself. This paper describes a binding from the Haskell programming language to basic ROS interfaces. The novelty of these bindings is that they allow for, and encourage, a higher level of abstraction in writing programs for robots that treat streams of values as first-class citizens. This approach makes the fusing, transforming, and filtering of streams fully generic and compositional while maintaining full compatibility with the existing ROS ecosystem. {\textcopyright} 2011 IEEE.},
annote = {A problem they notice is that box and arrow diagrams might use inputs that are produced asynchronously and simple box and arrow diagrams do not handle that
Objects in a symmetric monoidal category are tensor products of all available resources. Composability of system functions is achieved when the tensor product of output resources for one function equals the tensor product of input resource to another function.
This paper tries to address the problem of streaming commands to the robot and functional abstractions of that.
Wholemeal programming is writing the program using data structures conceptual prior to considering how the structures are comprised.
The most common approaches to semantic sensor fusion are best on analytical methods such as joint probabilities from empirical collections. However, given that sensors are models that project real-world objects into their chosen representations, does there exist a subdued relationship between representations of the same real-world objects that can be exploited? (General thesis)
A program is derived from a conceptual expectation. If we make this conceptual expectation explicit in a chosen representation and we derive programs from this representation, can we say that two programs are equal. Can we observe how similar they are to the parent representation? Can we identify a metric for exacting the number of extensions and exclusions? Does this say something about the relationship between two programs? Can the inverse of these extensions and exclusions be used to translate between programs? (General thesis)
For many businesses that find themselves rapidly prototyping many systems, they have a motivation to see how similar their systems are to one another to avoid re-inventing the wheel. They also may seek to design systems that are derivatives of larger concepts to increase reusability. If their sub functional components have the same conceptual signatures (including resource types) at some abstraction-level, then those are places in these systems where data can be shared and transferred. (General thesis)
This representation makes it evident where opportunities for parallelism and asynchrony are available.
This paper provides a syntax for capturing semantic design theory for robot manipulator programs.
The next paper provides an example of comparing an existing program to this conceptual representation and observing similarities and differences.},
author = {Cowley, Anthony and Taylor, Camillo J.},
doi = {10.1109/IROS.2011.6048682},
file = {:Users/angelineaguinaldo/Documents/Mendeley Desktop/Cowley, Taylor/2011/IEEE International Conference on Intelligent Robots and Systems/CowleyTaylorIROS11b(1).pdf:pdf},
isbn = {9781612844541},
journal = {IEEE International Conference on Intelligent Robots and Systems},
mendeley-groups = {Topics/Robotics/Robot Programming,Projects/Functional Interoperable Compiler (FIC)},
pages = {1048--1054},
title = {{Stream-oriented robotics programming: The design of roshask}},
year = {2011}
}


