file = {:Users/angelineaguinaldo/Documents/Mendeley Desktop/Bottazzi, Fonseca/2005/Proceedings of the Joint International Conference on Autonomic and Autonomous Systems and International Conference on Networking and Service./Bottazzi, Fonseca - 2005 - Off-line robot programming framework.pdf:pdf},
isbn = {0769524508},
journal = {Proceedings of the Joint International Conference on Autonomic and Autonomous Systems and International Conference on Networking and Services, ICAS/ICNS 2005},
keywords = {Framework,Off-line programming,Robot code generator},
