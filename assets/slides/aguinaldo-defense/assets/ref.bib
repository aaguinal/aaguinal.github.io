@article{Yang2016,
abstract = {Great attentions have been put on the programming technologies to construct robot software in both academic research and industry due to the increasingly wide applications of robots in various areas, and potential challenges resulting from the complexity of robot software. In the past years, diverse programming technologies and languages have been designed to support the development of robot software. However, with robot applications and requirements change, to develop robot software remains a great challenge, especially when robots are widely used in open environment and expected to provide better and friendly services for human beings. This paper aims at analyzing the technical requirements for designing robot programming language, presenting the roadmap of the robot programming language and discussing its trends and potential challenges.},
annote = {This paper summarizes the technical requirements for designing robot programming language and describes a roadmap for robot programming languages based on current trends and challenges.

Robot software is like a cyber-physical systems with new characteristics such as:
- Managing control with physical equipment and devices
- Dealing with non-functional requirements like real-time, security, safety
- Autonomous and self-adaptive because the environments are often open systems

Development of robotic programming languages began in 1973 up to present day.

Phase I (1970 - 1980) required:
* Establishing specific mechanisms for accurate control and error recovery
* Support sensory feedback in assembly operations

Phase II (1990s) required:
* Flexible mobility inspired by bionic and autonomy
* High-level programming approach for advance robot tasks
* Specification for behavior pattern to mimic adaptation

Phase III (2000-present) require:
* Use logic-based reasoning methods for task planning and decision-making
* Use agent-oriented techniques to program multiple robots at once

New challenges include:
* Communication mechanisms for distributed control for robot swarms
* Interoperability and portability of programs between different robot hardware platforms
* Autonomous or self-adaptive robot software needs methods to improve software dynamism and changeability
* Improve programming frameworks to promote resuability and well-structure programs},
author = {Yang, Shuo and Mao, Xinjun and Ge, Binbin and Yang, Sen},
doi = {10.1109/SMC.2015.69},
file = {:Users/angelineaguinaldo/Documents/Mendeley Desktop/Yang et al/2016/Proceedings - 2015 IEEE International Conference on Systems, Man, and Cybernetics, SMC 2015/Yang et al. - 2016 - The roadmap and challenges of robot programming languages.pdf:pdf},
isbn = {9781479986965},
journal = {Proceedings - 2015 IEEE International Conference on Systems, Man, and Cybernetics, SMC 2015},
keywords = {autonomous,robot programming language,robot software,self-adaptive},
pages = {328--333},
publisher = {IEEE},
title = {{The roadmap and challenges of robot programming languages}},
year = {2016}
}
