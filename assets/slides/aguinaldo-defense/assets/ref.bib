@article{Biggs2004,
abstract = {Robots have become significantly more powerful and intelligent over the last decade, and are moving in to more service oriented roles. As a result robots will more often be used by people with minimal technical skills and so there is a need for easier to use and more flexible programming systems. This paper reviews the current state of the art in robot programming systems. A distinction is made between manual and automatic programming systems. Manual systems require the user/programmer to create the robot program directly, by hand, while automatic systems generate a robot program as a result of interaction between the robot and the human; there are a variety of methods including learning, programming by demonstration and instructive systems.},
annote = {Manual vs. Automatic

-- Manual --
* User creates robot program directly
a. Manual Programming
1. Controller specific languages (KUKA KRL, ABB Rapids)
* lacks universal standards
* solution: write abstracted languages that remove controller specific programming, but become application specific
2. Generic procedural languages
* uses multi-purpose language
* still robot specific even with decent abstractions (often object oriented)
3. Behavior-based languages (FRP)
* describes how to react to conditions, not a procedure 
b. Graphical Programming (LEGO mindsotrm)
* require manual input to describe program flow
* easy to use, but not as flexible as text-based languages
1. Graph systems
2. Flowchart systems
3. Diagrammatic systems

-- Automatic --
* Code is generated from person interacting with it
a . learning systems
* examples and self-exploration of robot to learn
b. program by demonstration
1. Teach pendant
* record positions or move robot physically
* research questions: (1) product better robot program by combining multiple demonstrations by breaking down information into segments (2) improve demonstration options using multimodal communication
2. Gesture/voice/vision
c. instructive systems
* give instructions to exeecute in real-time



--- In Pointillist ---
This provides an outline of the currently available programming methods:
A. Manual
1. Manual Programming
a. Controller-specific languages (KUKA KRL, ABB Rapids)
b. Generic procedural languages (Python, C++)
c. Behavior-based programming (FRP, Frob, Yampa)
2. Graphical Programming
a. Graph systems
b. Flowchart systems
c. Diagrammatic systems
B. Automatic
1. Learning Systems
2. Program by demonstration
a. Teach pendant
b. Gesture/voice/vision
3. Instructive systems

The main challenges with manual programming are that the abstractions cause them to be controller or application specific. Translation frameworks have been introduced by Freud et. al.
The main challenges with automatic programming is that the programs are difficult to combine or re-used across demonstrations. Research is being done to fragment demonstrations into tasks that can be composed.},
author = {Biggs, Geoffrey and MacDonald, Bruce},
doi = {10.1109/robot.2001.932554},
file = {:Users/angelineaguinaldo/Documents/Mendeley Desktop/Biggs, MacDonald/2004/Proceedings - IEEE International Conference on Robotics and Automation/Bredenfeld, Indiveri - 2001 - Robot behavior engineering using DD-Designer.pdf:pdf},
issn = {10504729},
journal = {Proceedings - IEEE International Conference on Robotics and Automation},
keywords = {Behavior-based robotics,Programming and architecture,Rapid prototyping and design automation,Robotic software development framework},
title = {{A Survey of Robot Programming Systems}},
year = {2004}
}
@article{Chalvatzaki2023,
abstract = {Long-horizon task planning is essential for the development of intelligent assistive and service robots. In this work, we investigate the applicability of a smaller class of large language models (LLMs), specifically GPT-2, in robotic task planning by learning to decompose tasks into subgoal specifications for a planner to execute sequentially. Our method grounds the input of the LLM on the domain that is represented as a scene graph, enabling it to translate human requests into executable robot plans, thereby learning to reason over long-horizon tasks, as encountered in the ALFRED benchmark. We compare our approach with classical planning and baseline methods to examine the applicability and generalizability of LLM-based planners. Our findings suggest that the knowledge stored in an LLM can be effectively grounded to perform long-horizon task planning, demonstrating the promising potential for the future application of neuro-symbolic planning methods in robotics.},
archivePrefix = {arXiv},
